    "from itertools import combinations_with_replacement\n",
    "s, k = input().split()\n",
    "s= sorted(s)\n",
    "k = int(k)\n",
    "c = combinations_with_replacement(s,k)\n",
    "for i in c:\n",
    "    print(''.join(i))"
